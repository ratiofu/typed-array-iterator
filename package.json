{
  "name": "@ratiofu/type-array-proxy",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "generate-fixtures": "bun run src/generate-fixture-data.ts",
    "check": "biome check --write .",
    "quality": "bun check && bun run test:coverage",
    "quality:all": "time (bun quality && bun generate-fixtures 1 && bun src/index.ts)"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@ngneat/falso": "8.0.2",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "5.9.2"
  }
}
