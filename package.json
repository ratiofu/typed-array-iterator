{
  "name": "@ratiofu/type-array-proxy",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "generate-fixtures": "bun run src/generateFixtureData.ts",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "quality": "bun typecheck && bun check && bun run test",
    "quality:all": "time (bun quality && bun generate-fixtures 1 && bun src/index.ts)",
    "benchmark:small": "bun --expose-gc src/benchmark/index.ts 20000",
    "benchmark:medium": "bun --expose-gc src/benchmark/index.ts 100000",
    "benchmark:large": "bun --expose-gc src/benchmark/index.ts 500000"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@ngneat/falso": "8.0.2",
    "@types/bun": "latest",
    "mitata": "1.0.34"
  },
  "peerDependencies": {
    "typescript": "5.9.2"
  }
}
